Men's Results {{ event.EventName }}
            <table>
                <tr>
                  <th>Name</th>
                  <th>Meet</th>
                  <th>Mark</th>
                </tr>
         
              
              {% for performance in performancesmen %}
  
                <tr> 
                    
                    
                            <td>
                              {{forloop.counter}}.
                              {% for athlete in performance.AthleteID.all %}
                              <p><a href ="{% url 'update-athlete' athlete.id %}">  {{athlete.Athlete}} </a></p>
                               
                               {% endfor %} 
                              </td> 
                             
                            <td>{{performance.MeetID.MeetName}}</td> 
                            <td>{{performance.Mark}} </td>
  
                      
                    
                </tr>
            
            
                       
                
                        
                
                {% endfor %}
            </table>
